      "clientRoles": {
        "realm-management": [
          "view-clients",
          "view-users",
          "manage-authorization",
          "manage-users",
          "manage-clients"
        ],
        "uk-ets-registry-api": ["uma_protection"]
      }
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "ets_user",
        "description": "ETS User privileges"
      }
    ],
    "client": {
      "uk-ets-registry-api": [
        {
          "name": "system-administrator",
          "description": "A role for system administration actions",
          "composite": false,
          "clientRole": true,
          "attributes": {}
        },
        {
          "name": "authority-user",
          "description": "A role for authority user actions",
          "composite": false,
          "clientRole": true,
          "attributes": {}
        },
        {
          "name": "senior-registry-administrator",
          "composite": false,
          "clientRole": true,
          "attributes": {}
        },
        {
          "name": "junior-registry-administrator",
          "composite": false,
          "clientRole": true,
          "attributes": {}
        },
        {
          "name": "readonly-administrator",
          "composite": false,
          "clientRole": true,
          "attributes": {}
        },
        {
          "name": "verifier",
          "composite": false,
          "clientRole": true,
          "attributes": {}
        },
        {
          "name": "authorized-representative",
          "composite": false,
          "clientRole": true,
          "attributes": {}
        }
      ]
    }
  },
  "defaultRoles": ["offline_access", "ets_user"],
  "clients": [
    {
      "clientId": "uk-ets-web-app",
      "enabled": true,
      "adminUrl": "http://localhost:4200",
      "baseUrl": "http://localhost:4200",
      "publicClient": true,
      "consentRequired": false,
      "fullScopeAllowed": true,
      "directAccessGrantsEnabled": true,
      "redirectUris": ["http://localhost:4200/*"],
      "webOrigins": ["*"],
      "protocolMappers": [
        {
          "id": "fc18ce7e-a6bb-4b4d-bc82-171c0e30c40f",
          "name": "User state mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "user.attribute": "state",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "state",
            "userinfo.token.claim": "true"
          }
        },
        {
          "id": "4d5c027a-2b2f-49c1-8733-f70567c9a9a5",
          "name": "User urid mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "userinfo.token.claim": "true",
            "user.attribute": "urid",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "urid",
            "jsonType.label": "String"
          }
        },
        {
          "id": "fd917c94-2a77-4154-939a-87c970a51d74",
          "name": "Audience for uk-ets-registry-api",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "consentRequired": false,
          "config": {
            "included.client.audience": "uk-ets-registry-api",
            "id.token.claim": "false",
            "access.token.claim": "true"
          }
        }
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    },
    {
      "clientId": "uk-ets-registry-api",
      "secret": "4eeda6c4-0a5c-427d-9ec9-743b18d5976f",
      "enabled": true,
      "baseUrl": "http://localhost:8080/api-registry",
      "authorizationServicesEnabled": true,
      "redirectUris": ["http://localhost:4200/"],
      "webOrigins": ["*"],
      "protocolMappers": [
        {
          "name": "Client IP Address",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usersessionmodel-note-mapper",
          "consentRequired": false,
          "config": {
            "user.session.note": "clientAddress",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "clientAddress",
            "jsonType.label": "String"
          }
        },
        {
          "name": "Client Host",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usersessionmodel-note-mapper",
          "consentRequired": false,
          "config": {
            "user.session.note": "clientHost",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "clientHost",
            "jsonType.label": "String"
          }
        },
        {
          "name": "Client ID",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usersessionmodel-note-mapper",
          "consentRequired": false,
          "config": {
            "user.session.note": "clientId",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "clientId",
            "jsonType.label": "String"
          }
        }
      ],
      "authorizationSettings": {
        "allowRemoteResourceManagement": true,
        "policyEnforcementMode": "PERMISSIVE"
      }
    }
  ],
  "attributes": {
    "bruteForceProtected": "true",
