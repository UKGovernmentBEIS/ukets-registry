{
  "name": "uk-ets-registry-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:hmr": "ng serve --hmr",
    "start aot": "ng serve --aot",
    "start-es5": "ng serve --configuration es5",
    "prebuild": "eslint . --quiet --cache",
    "build": "npx jest --coverage[=true] --maxWorkers=2 && ng build --configuration production",
    "build-quickly": "ng build",
    "build-quickly-prod": "ng build --configuration production",
    "test": "jest --coverage[=true]",
    "test watch": "jest --watch",
    "lint": "eslint .",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e -c local",
    "e2e-remote": "ng e2e -c remote",
    "sonar": "sonar-scanner",
    "build:stats-analyze": "node --max_old_space_size=8192 $(which ng) build --configuration production --stats-json && webpack-bundle-analyzer dist/uk-ets-registry-web/stats-es2015.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.9",
    "@angular/cdk": "16.2.8",
    "@angular/common": "16.2.9",
    "@angular/compiler": "16.2.9",
    "@angular/core": "16.2.9",
    "@angular/forms": "16.2.9",
    "@angular/localize": "16.2.9",
    "@angular/platform-browser": "16.2.9",
    "@angular/platform-browser-dynamic": "16.2.9",
    "@angular/router": "16.2.9",
    "@hmcts/frontend": "0.0.50-alpha",
    "@ng-bootstrap/ng-bootstrap": "15.1.1",
    "@ngrx/effects": "16.3.0",
    "@ngrx/router-store": "16.3.0",
    "@ngrx/schematics": "16.3.0",
    "@ngrx/store": "16.3.0",
    "@ngrx/store-devtools": "16.3.0",
    "@ngx-pwa/local-storage": "16.3.0",
    "angular-web-storage": "16.0.0",
    "core-js": "3.33.0",
    "crypto-js": "4.1.1",
    "dayjs": "^1.11.10",
    "dialog-polyfill": "^0.5.6",
    "file-saver": "2.0.5",
    "google-libphonenumber": "3.2.33",
    "govuk-frontend": "3.14.0",
    "hi-base32": "^0.5.1",
    "immer": "9.0.6",
    "jest-jasmine2": "^29.0.2",
    "jimp": "^0.16.1",
    "jsqr": "^1.4.0",
    "keycloak-angular": "14.1.0",
    "keycloak-js": "21.1.2",
    "ng-inline-svg-2": "15.0.1",
    "ngx-page-scroll-core": "11.0.0",
    "ngx-quill": "16.1.1",
    "quill": "1.3.7",
    "rxjs": "7.5.6",
    "speakeasy": "^2.0.0",
    "superagent": "8.1.2",
    "tslib": "2.3.1",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "16.0.1",
    "@angular-devkit/build-angular": "16.2.6",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "16.2.6",
    "@angular/compiler-cli": "16.2.9",
    "@angular/language-service": "16.2.9",
    "@babel/core": "7.23.2",
    "@keycloak/keycloak-admin-client": "19.0.1",
    "@storybook/addon-actions": "7.4.6",
    "@storybook/addon-links": "7.4.6",
    "@storybook/angular": "^7.4.6",
    "@storybook/builder-webpack5": "^7.4.5",
    "@storybook/manager-webpack5": "^6.4.18",
    "@types/chai": "4.2.11",
    "@types/cucumber": "6.0.1",
    "@types/jest": "29.0.0",
    "@types/quill": "1.3.7",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "axe-webdriverjs": "2.3.0",
    "babel-jest": "29.7.0",
    "babel-loader": "8.2.3",
    "chai": "4.2.0",
    "commit-message-validator": "1.0.2",
    "cucumber": "6.0.7",
    "cucumber-html-reporter": "7.1.1",
    "cucumber-tsflow": "4.1.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsdoc": "46.8.2",
    "eslint-plugin-ngrx": "2.1.4",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-rxjs": "^5.0.2",
    "husky": "4.3.8",
    "jasmine-marbles": "^0.9.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-preset-angular": "^13.1.2",
    "jest-sonar-reporter": "2.0.0",
    "jira-connector": "3.1.0",
    "mailhog": "4.16.0",
    "openid-client": "4.9.1",
    "prettier": "2.4.1",
    "pretty-quick": "3.1.1",
    "protractor": "^7.0.0",
    "protractor-cucumber-framework": "9.8.0",
    "protractor-http-client": "1.0.4",
    "shelljs": "^0.8.5",
    "sonar-scanner": "3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "10.4.0",
    "typescript": "~5.1.6",
    "uuid": "9.0.1",
    "webpack-bundle-analyzer": "4.5.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commit-message-validator",
      "pre-commit": "pretty-quick --staged --bail",
      "pre-push": "eslint . --quiet && ng build --configuration production"
    }
  },
  "jestSonar": {
    "reportPath": "reports/tests/jest",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "config": {
    "commit-message-validator": {
      "pattern": "^\\[UKETS-[0-9]+\\]\\s[A-Z][A-Za-z0-9,;.\\-()#/:@='\\s]+[.]$",
      "errorMessage": "Commit message must follow the format: [UKETS-11111] The message goes here."
    }
  }
}
