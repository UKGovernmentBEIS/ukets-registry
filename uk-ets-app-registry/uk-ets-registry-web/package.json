{
  "name": "uk-ets-registry-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:hmr": "ng serve --hmr",
    "start aot": "ng serve --aot",
    "start-es5": "ng serve --configuration es5",
    "prebuild": "eslint . --quiet --cache",
    "build": "npx jest --coverage --maxWorkers=2 && ng build --configuration production",
    "build-quickly": "ng build",
    "build-quickly-prod": "ng build --configuration production",
    "test": "jest --coverage",
    "test watch": "jest --watch",
    "lint": "eslint .",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e -c local",
    "e2e-remote": "ng e2e -c remote",
    "sonar": "sonar-scanner",
    "build:stats-analyze": "node --max_old_space_size=8192 $(which ng) build --configuration production --stats-json && webpack-bundle-analyzer dist/uk-ets-registry-web/stats-es2015.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.13",
    "@angular/cdk": "18.2.13",
    "@angular/common": "18.2.13",
    "@angular/compiler": "18.2.13",
    "@angular/core": "18.2.13",
    "@angular/forms": "18.2.13",
    "@angular/localize": "18.2.13",
    "@angular/platform-browser": "18.2.13",
    "@angular/platform-browser-dynamic": "18.2.13",
    "@angular/router": "18.2.13",
    "@hmcts/frontend": "0.0.50-alpha",
    "@ng-bootstrap/ng-bootstrap": "17.0.1",
    "@ngrx/effects": "18.1.1",
    "@ngrx/router-store": "18.1.1",
    "@ngrx/schematics": "18.1.1",
    "@ngrx/store": "18.1.1",
    "@ngrx/operators": "18.1.1",
    "@ngrx/store-devtools": "18.1.1",
    "@ngx-pwa/local-storage": "18.0.0",
    "angular-web-storage": "17.0.0",
    "core-js": "3.35.0",
    "crypto-js": "4.1.1",
    "dayjs": "^1.11.10",
    "dialog-polyfill": "^0.5.6",
    "file-saver": "2.0.5",
    "google-libphonenumber": "3.2.38",
    "govuk-frontend": "5.8.0",
    "hi-base32": "^0.5.1",
    "immer": "9.0.6",
    "jest-jasmine2": "^29.0.2",
    "jimp": "^0.16.1",
    "jsqr": "^1.4.0",
    "keycloak-angular": "16.1.0",
    "keycloak-js": "26.1.0",
    "ng-inline-svg-2": "15.0.1",
    "ngx-page-scroll-core": "13.0.0",
    "ngx-quill": "16.1.1",
    "quill": "1.3.7",
    "rxjs": "7.8.1",
    "speakeasy": "^2.0.0",
    "superagent": "8.1.2",
    "tslib": "2.3.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-builders/jest": "18.0.0",
    "@angular-devkit/build-angular": "18.2.12",
    "@angular-eslint/builder": "19.0.2",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/schematics": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/cli": "18.2.12",
    "@angular/compiler-cli": "18.2.13",
    "@angular/language-service": "18.2.13",
    "@babel/core": "7.23.2",
    "@keycloak/keycloak-admin-client": "26.0.7",
    "@storybook/addon-actions": "7.6.10",
    "@storybook/addon-links": "7.6.10",
    "@storybook/angular": "^7.6.10",
    "@storybook/builder-webpack5": "^7.4.5",
    "@storybook/manager-webpack5": "^6.4.18",
    "@types/chai": "4.2.11",
    "@types/cucumber": "6.0.1",
    "@types/jest": "29.5.11",
    "@types/quill": "1.3.7",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "axe-webdriverjs": "2.3.0",
    "babel-jest": "29.7.0",
    "babel-loader": "8.2.3",
    "chai": "4.2.0",
    "commit-message-validator": "1.0.2",
    "cucumber": "6.0.7",
    "cucumber-html-reporter": "7.1.1",
    "cucumber-tsflow": "4.4.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.0.2",
    "eslint-plugin-ngrx": "2.1.4",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-rxjs": "^5.0.2",
    "husky": "4.3.8",
    "jasmine-marbles": "^0.9.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-preset-angular": "^14.0.0",
    "jest-sonar-reporter": "2.0.0",
    "jira-connector": "3.1.0",
    "mailhog": "4.16.0",
    "openid-client": "4.9.1",
    "prettier": "3.3.3",
    "pretty-quick": "4.0.0",
    "protractor": "^7.0.0",
    "protractor-cucumber-framework": "9.10.0",
    "protractor-http-client": "1.0.4",
    "shelljs": "^0.8.5",
    "sonar-scanner": "3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "10.4.0",
    "typescript": "~5.5.4",
    "uuid": "9.0.1",
    "webpack-bundle-analyzer": "4.5.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commit-message-validator",
      "pre-commit": "pretty-quick --staged --bail",
      "pre-push": "eslint . --quiet && ng build --configuration production"
    }
  },
  "jestSonar": {
    "reportPath": "reports/tests/jest",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "config": {
    "commit-message-validator": {
      "pattern": "^\\[UKETS-[0-9]+\\]\\s[A-Z][A-Za-z0-9,;.\\-()#/:@='\\s]+[.]$",
      "errorMessage": "Commit message must follow the format: [UKETS-11111] The message goes here."
    }
  }
}
